.breadcrumb-map {
  background: white;
  padding-top 15px;
  padding-bottom 15px;
  border-bottom-right-radius 4px;
  border-top-right-radius 4px;
  box-shadow: 1px 3px 0px 2px rgba(0,0,0,.1);
  @media $max-xl {
    width: 100vw;
    margin-left: -15px;
    margin-right: -15px;
    display flex;
    justify-content center
    align-content center;
    padding-top 5px;
    padding-bottom 5px;
  }
  a {
    &:not(:last-child) {
      color: #a3a3a2
    }
    &:last-child {
      color $dark;
    }
  }
}
